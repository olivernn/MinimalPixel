<h1>Listing images</h1>

<table>
  <tr>
  </tr>

<% for image in @images %>
  <tr>
    <td><%= link_to 'Show', [@project, image] %></td>
    <td><%= link_to 'Edit', edit_project_image_path(@project, image) %></td>
    <td><%= link_to 'Destroy', [@project, image], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New image', new_project_image_path(@project) %>

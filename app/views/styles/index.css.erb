#leftcolumn, #rightcolumn, #header, #footer{
	background-color:<%= @style.theme.background_colour%>;
}

body{
	color:<%= @style.theme.text_colour %>;
}

h1, h2, h3, h4, h5{
	color:<%= @style.palette.foreground %>;
}

a{
	color:<%= @style.palette.foreground %>;
}

a:visited{
	color:<%= @style.palette.foreground_light %>;
}
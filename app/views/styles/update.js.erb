var timestamp = new Date().getTime();
$("#user_styles").replaceWith("<link href='/styles/<%= @style.id %>.css?time=" + timestamp + "' id='user_styles' rel='stylesheet' media='screen' type='text/css'/>");
$("#user_font").replaceWith("");
$("h1, h2, h3, h4").sifr({font:'<%= @style.font.font.url %>', color:'<%= @style.heading_colour %>'});
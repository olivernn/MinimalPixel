<h1>Listing videos</h1>

<table>
  <tr>
  </tr>

<% for video in @videos %>
  <tr>
    <td><%= link_to 'Show', [@project, video] %></td>
    <td><%= link_to 'Edit', edit_project_video_path(@project, video) %></td>
    <td><%= link_to 'Destroy', [@project, video], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video', new_project_video_path(@project) %>
